{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/admin",
      "dest": "/admin-dashboard.html"
    },
    {
      "src": "/user",
      "dest": "/user.html"
    },
    {
      "src": "/chat",
      "dest": "/live-chat.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/register",
      "dest": "/register.html"
    },
    {
      "src": "/order",
      "dest": "/order.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}